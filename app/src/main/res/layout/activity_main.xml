<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".MainActivity">

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/circleImageView"
        android:layout_width="160dp"
        android:layout_height="250dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="481dp"
        android:src="@drawable/u"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/circleImageView2"
        android:layout_width="25dp"
        android:layout_height="250dp"
        android:layout_marginStart="3dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="4dp"
        android:src="@drawable/ic_baseline_person_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.050"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.100" />

    <ImageView
        android:id="@+id/circleImageView3"
        android:layout_width="25dp"
        android:layout_height="250dp"
        android:layout_marginStart="3dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="4dp"
        android:src="@drawable/ic_baseline_email_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.050"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.340" />

    <ImageView
        android:id="@+id/circleImageView4"
        android:layout_width="25dp"
        android:layout_height="250dp"
        android:layout_marginStart="3dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="4dp"
        android:src="@drawable/ic_baseline_school_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.050"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.580" />

    <TextView
        android:id="@+id/teks1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nama"
        android:textSize="12dp"
        tools:ignore="MissingConstraints"
        android:paddingTop="220dp"
        android:paddingStart="65dp"
        android:textColor="#808080" />


    <TextView
        android:id="@+id/teks2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sofyan Sauri"
        android:textSize="14dp"
        tools:ignore="MissingConstraints"
        android:paddingTop="240dp"
        android:paddingStart="65dp"
        android:textColor="#000000"


        />



    <TextView
        android:id="@+id/teks3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email"
        android:textSize="12dp"
        tools:ignore="MissingConstraints"
        android:paddingTop="280dp"
        android:paddingStart="65dp"
        android:textColor="#808080" />

    <TextView
        android:id="@+id/teks4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sofyans21@gmail.com"
        android:textSize="14dp"
        tools:ignore="MissingConstraints"
        android:paddingTop="300dp"
        android:paddingStart="65dp"
        android:textColor="#000000"
        />

    <TextView
        android:id="@+id/teks5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jurusan"
        android:textSize="12dp"
        tools:ignore="MissingConstraints"
        android:paddingTop="340dp"
        android:paddingStart="65dp"
        android:textColor="#808080" />

    <TextView
        android:id="@+id/teks6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rekayasa Perangkat Lunak"
        android:textSize="14dp"
        tools:ignore="MissingConstraints"
        android:paddingTop="360dp"
        android:paddingStart="65dp"
        android:textColor="#000000" />

</androidx.constraintlayout.widget.ConstraintLayout>